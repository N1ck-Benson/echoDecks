datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Deck {
  id Int @id @default(autoincrement())
  title String
  lemmas String[]
  flashcards String
  src String
  dst String
  createdAt DateTime @default(now())
  hasFlashcards Boolean @default(true)
  isLearned Boolean @default(false)
}
